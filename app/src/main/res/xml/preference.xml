<?xml version="1.0" encoding="utf-8"?>

<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    android:title="환경 설정">


    <PreferenceScreen android:title="@string/settings_alarm"

        android:summary="소리및 진동알림을 설정할 수 있습니다.">
        <intent android:targetPackage="com.android.settings"
            android:targetClass="com.android.settings.SoundSettings"/>

    </PreferenceScreen>

    <PreferenceScreen android:title = "@string/settings_lang"
        android:summary="@string/settings_language">
        <intent android:targetPackage="com.android.settings"
            android:targetClass="com.android.settings.LanguageSettings"/>
    </PreferenceScreen>
</PreferenceScreen>
